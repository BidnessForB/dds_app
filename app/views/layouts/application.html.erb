<!DOCTYPE html>
<html>
<head>
  <title>Devil Dog Squadron :: </title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
  <meta name="viewport" content="initial-scale=1, maximum-scale=1">
</head>
<body>

<div id="container">

<nav>
  <%= render 'layouts/nav' %>
</nav>



<div class="main">
  
  <article>
    <% if notice %>
      <div id="notice"><%= notice %></div>
    <% end %>

    <%= yield %>
  </article>

</div>

</div>

<footer>
    <%= render 'layouts/footer' %>
</footer>

</div>

</body>
</html>