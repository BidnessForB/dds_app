<div class="content-left">

<h1>Listing videos</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Description</th>
    <th>Date</th>
    <th>Source</th>
    <th>Ext url</th>
    <th>Publish</th>
    <th>Members only</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @videos.each do |video| %>
  <tr>
    <td><%= video.title %></td>
    <td><%= video.description %></td>
    <td><%= video.date %></td>
    <td><%= video.source %></td>
    <td><%= video.ext_url %></td>
    <td><%= video.publish %></td>
    <td><%= video.members_only %></td>
    <td><%= link_to 'Show', video %></td>
    <td><%= link_to 'Edit', edit_video_path(video) %></td>
    <td><%= button_to 'Destroy', video, :confirm => 'Are you sure?', :method => :delete %></td></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Video', new_video_path %>

</div>